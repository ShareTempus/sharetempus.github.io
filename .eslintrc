{
  "extends": [
    "airbnb"
  ],
  "globals": {
    "document": true,
    "navigator": true,
    "window": true
  },
  "rules": {
    "max-len": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/media-has-caption": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-vars": ["error", { "varsIgnorePattern": "h" }]
  }
}
